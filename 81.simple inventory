#include <stdio.h>
struct item {
    int id, qty;
};

int main() {
    struct item i[3];
    int n=0, ch;

    printf("1.Add 2.Display\n");
    scanf("%d",&ch);

    if(ch==1) {
        scanf("%d %d",&i[n].id,&i[n].qty);
        n++;
    }

    if(ch==2) {
        for(int j=0;j<n;j++)
            printf("ID:%d Qty:%d\n",i[j].id,i[j].qty);
    }
    return 0;
}
