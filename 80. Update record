#include <stdio.h>
struct emp {
    int id;
    char name[20];
};

int main() {
    struct emp e[2]={{1,"Ram"},{2,"Sam"}};
    int i,sid;

    scanf("%d",&sid);

    for(i=0;i<2;i++) {
        if(e[i].id==sid)
            scanf("%s",e[i].name);
    }

    for(i=0;i<2;i++)
        printf("%d %s\n",e[i].id,e[i].name);
    return 0;
}
