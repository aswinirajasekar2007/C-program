#include <stdio.h>
int main() {
    int qty;
    float price, total;
    printf("Enter quantity: ");
    scanf("%d", &qty);
    printf("Enter price per item: ");
    scanf("%f", &price);
    total = qty * price;
    printf("Total Bill = %.2f", total);
    return 0;
}


output:
Enter quantity: 5
Enter price per item: 40
Total Bill = 200.00
