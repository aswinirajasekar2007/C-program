#include <stdio.h>
#include <string.h>
int main() {
    char mob[15];
    int i, valid = 1;

    scanf("%s", mob);

    if (strlen(mob) != 10)
        valid = 0;
    else {
        for (i = 0; i < 10; i++) {
            if (mob[i] < '0' || mob[i] > '9') {
                valid = 0;
                break;
            }
        }
    }

    if (valid)
        printf("Valid Mobile Number");
    else
        printf("Invalid Mobile Number");

    return 0;
}
